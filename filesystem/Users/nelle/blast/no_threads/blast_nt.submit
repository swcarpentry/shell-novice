#!/bin/sh
#SBATCH --nodes=1
#SBATCH --ntasks-per-node=1
#SBATCH --mem=3gb
#SBATCH --time=00:05:00
#SBATCH --job-name=blast_nt
#SBATCH --error=blast.nt.%J.err
#SBATCH --output=blast.nt.%J.out
#SBATCH --reservation=springuno

module load blast/2.2.29
module load biodata/1.0

cp $BLAST/nt.00.* /tmp
cp yeast.nt.fasta /tmp

blastn -db /tmp/nt.00 -query /tmp/yeast.nt.fasta -out /tmp/blast_nt.alignments
cp /tmp/blast_nt.alignments .
